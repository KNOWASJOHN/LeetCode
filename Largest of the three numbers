# Largest of the three numbers

print("Enter Three Values Seperated By Spaces:",end="")
num_1,num_2,num_3 = map(int, input().split())
if (num_1>num_2 and num_1>num_3):
    print(f"{num_1} is the Greatest") 
else:
    if(num_2 > num_3):
        print(f"{num_2} is the Greatest")
    else:
        print(f"{num_3} is the Greatest")
